{
  "name": "frontend_boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.7.0",
    "@material-ui/icons": "^3.0.1",
    "@types/jest": "23.3.10",
    "@types/node": "10.12.15",
    "@types/react": "16.7.17",
    "@types/react-dom": "16.0.11",
    "apollo-boost": "^0.1.22",
    "graphql": "^14.0.2",
    "react": "^16.6.3",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "typeface-roboto": "0.0.54",
    "typescript": "3.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gen:clean": "find **/__generated__ -delete || true",
    "gen:schema": "apollo schema:download --endpoint http://localhost:4000/",
    "gen:types": "apollo client:codegen --target typescript --endpoint http://localhost:4000/",
    "generate": "npm run gen:clean && npm run gen:types"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/graphql": "^14.0.3",
    "@types/react-router-dom": "^4.3.1"
  }
}
